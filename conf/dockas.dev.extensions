[ dockas_dev ]
nsCertType              = server
keyUsage                = digitalSignature,nonRepudiation,keyEncipherment
extendedKeyUsage        = serverAuth
subjectKeyIdentifier    = hash
authorityKeyIdentifier  = keyid,issuer
subjectAltName          = @dockas_dev_subject

[ dockas_dev_subject ]
DNS.1 = dockas.dev
DNS.2 = www.dockas.dev
DNS.3 = blog.dockas.dev
DNS.4 = api.dockas.dev
DNS.5 = socket.dockas.dev
DNS.6 = file.dockas.dev
